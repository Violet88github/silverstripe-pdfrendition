{
    "name": "symbiote/silverstripe-pdfrendition",
    "description": "A module that makes use of the Flying Saucer XHTML renderer project to create PDFs from XHTML pages.",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "pdf"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Marcus Nyeholt",
            "email": "marcus@symbiote.com.au"
        }
    ],
    "require": {
        "silverstripe/framework": "^4 || ^5"
    },
    "replace": {
        "silverstripe-australia/pdfrendition":"1.*"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        },
        "expose": [
            "client"
        ]
    },
    "autoload": {
        "psr-4": {
            "Symbiote\\PdfRendition\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/vendor-plugin": true
        }
    }
}
